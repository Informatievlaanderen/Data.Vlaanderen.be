  <owl:ObjectProperty rdf:about="http://data.vlaanderen.be/ns/persoon#heeftOverlijden">
    <rdfs:label xml:lang="nl">heeft overlijden</rdfs:label>
    <ns0:usageNote xml:lang="nl"> </ns0:usageNote>
    <rdfs:comment xml:lang="nl">Verwijst naar de overlijdensgegevens vd persoon.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/person#Person"/>
