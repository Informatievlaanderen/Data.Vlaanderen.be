  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://data.vlaanderen.be/ns/persoon#isHoofdVan">
    <rdfs:label xml:lang="nl">is hoofd van</rdfs:label>
    <ns0:usageNote xml:lang="nl"> </ns0:usageNote>
    <rdfs:comment xml:lang="nl">Persoon die standaard het gezin vertegenwoordigt.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/person#Person"/>
    <rdfs:isDefinedBy rdf:resource="http://data.vlaanderen.be/ns/persoon"/>
    <rdfs:range rdf:resource="http://data.vlaanderen.be/ns/persoon#Gezin"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://data.vlaanderen.be/ns/persoon#isLidVan">
        <rdfs:label xml:lang="nl">is lid van</rdfs:label>
        <ns0:usageNote xml:lang="nl"> </ns0:usageNote>
        <rdfs:comment xml:lang="nl">Persoon die tot een gezin behoort.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/person#Person"/>
        <rdfs:isDefinedBy rdf:resource="http://data.vlaanderen.be/ns/persoon"/>
        <rdfs:range rdf:resource="http://data.vlaanderen.be/ns/persoon#Gezin"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/org#memberOf"/>
