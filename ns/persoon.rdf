      <owl:Class rdf:about="http://data.vlaanderen.be/ns/persoon#Overlijden">
        <rdfs:label xml:lang="nl">Overlijden</rdfs:label>
        <ns0:usageNote xml:lang="nl"> </ns0:usageNote>
        <rdfs:comment xml:lang="nl">Het doodgaan vd Persoon.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.vlaanderen.be/ns/persoon"/>
        <rdfs:subClassOf rdf:resource="http://data.vlaanderen.be/ns/persoon#Persoonsgebeurtenis"/>
        <owl:equivalentClass xml:lang="en">http://purl.org/vocab/bio/0.1/Death</owl:equivalentClass>
      </owl:Class>
    </rdfs:range>

    <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/bio/0.1/death"/>
