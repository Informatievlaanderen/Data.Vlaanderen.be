  <owl:ObjectProperty rdf:about="http://data.vlaanderen.be/ns/persoon#gezinsrelatietype">
    <rdfs:label xml:lang="nl">gezinsrelatietype</rdfs:label>
    <ns0:usageNote xml:lang="nl">Wordt typisch bepaald tov het gezinshoofd. Bv als de vader gezinshoofd is en een gezinslid is zoon, dan zou als de grootvader gezinshoofd was datzelfde gezinslid kleinzoon zijn.</ns0:usageNote>
    <rdfs:comment xml:lang="nl">Aard vd relatie.</rdfs:comment>
    <rdfs:domain>
      <owl:Class rdf:about="http://data.vlaanderen.be/ns/persoon#Gezinsrelatie">
        <rdfs:label xml:lang="nl">Gezinsrelatie</rdfs:label>
        <ns0:usageNote xml:lang="nl">Bv echtgenoot, zoon, schoonmoeder.</ns0:usageNote>
        <rdfs:comment xml:lang="nl">Relatie tussen leden van eenzelfde gezin.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.vlaanderen.be/ns/persoon"/>
        <rdfs:subClassOf rdf:resource="http://data.vlaanderen.be/ns/persoon#Persoonsrelatie"/>
      </owl:Class>
    </rdfs:domain>

    <rdfs:isDefinedBy rdf:resource="http://data.vlaanderen.be/ns/persoon"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://data.vlaanderen.be/ns/persoon#heeftBurgerlijkeStaat">
    <rdfs:label xml:lang="nl">heeft burgerlijke staat</rdfs:label>
    <ns0:usageNote xml:lang="nl"> </ns0:usageNote>
    <rdfs:comment xml:lang="nl">Burgerlijke staat vd Persoon.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/person#Person"/>
    <rdfs:isDefinedBy rdf:resource="http://data.vlaanderen.be/ns/persoon"/>
    <rdfs:range rdf:resource="http://data.vlaanderen.be/ns/persoon#BurgerlijkeStaat"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://data.vlaanderen.be/ns/persoon#heeftGeboorte">
    <rdfs:label xml:lang="nl">heeft geboorte</rdfs:label>
    <ns0:usageNote xml:lang="nl"> </ns0:usageNote>
    <rdfs:comment xml:lang="nl">Verwijst naar de geboortegegevens vd persoon.</rdfs:comment>
