  </owl:ObjectProperty>

  <rdf:Property rdf:about="http://data.vlaanderen.be/ns/persoon#gezinsadres">
    <rdfs:label xml:lang="nl">gezinsadres</rdfs:label>
    <ns0:usageNote xml:lang="nl">Dikwijls een criterium om te bepalen of personen deel uitmaken van eenzelfde gezin.</ns0:usageNote>
    <rdfs:comment xml:lang="nl">Verblijfplaats vh gezin.</rdfs:comment>
    <rdfs:domain rdf:resource="http://data.vlaanderen.be/ns/persoon#Gezin"/>
