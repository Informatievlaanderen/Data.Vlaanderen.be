    <rdfs:domain rdf:resource="http://data.vlaanderen.be/ns/persoon#Persoonsgebeurtenis"/>
    <rdfs:isDefinedBy rdf:resource="http://data.vlaanderen.be/ns/persoon"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/bio/0.1/date"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://data.vlaanderen.be/ns/persoon#datumVanAfstamming">
