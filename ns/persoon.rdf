    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/type"/>
  </owl:ObjectProperty>

  <rdf:Property rdf:about="http://data.vlaanderen.be/ns/persoon#verblijfsadres">
