    <rdfs:comment xml:lang="en">Formally accepted topic to be discussed in a meeting.</rdfs:comment>
    <rdfs:comment xml:lang="nl">Formeel aanvaard te behandelen onderwerp op een zitting.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://data.vlaanderen.be/ns/besluit"/>
