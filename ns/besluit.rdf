  </owl:Class>
  <owl:ObjectProperty rdf:about="http://data.vlaanderen.be/ns/besluit#Agendapunt.type">
    <rdfs:comment xml:lang="en">The type of the agenda item.</rdfs:comment>
    <rdfs:comment xml:lang="nl">Type van het agendapunt.</rdfs:comment>
    <rdfs:domain rdf:resource="http://data.vlaanderen.be/ns/besluit#Agendapunt"/>
