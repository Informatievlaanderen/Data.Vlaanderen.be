    rdfs:isDefinedBy <http://data.vlaanderen.be/ns/organisatie> ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf rov:orgType .

<http://data.vlaanderen.be/ns/organisatie#redenStopzetting> a owl:ObjectProperty ;
